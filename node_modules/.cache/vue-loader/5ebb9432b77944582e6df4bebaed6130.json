pt>\r\nimport firebase from '@/firebase/firestore'\r\n\r\nexport default {\r\n    name: 'PickupSalon',\r\n    data () {\r\n        return {\r\n            arrSalons: []\r\n        }\r\n    },\r\n    created () {\r\n        var db = firebase.firestore()\r\n        /* サロンデータの取得 */\r\n        var docRef = db.collection('salons')\r\n        docRef.get().then(DocumentSnapshot => {\r\n            DocumentSnapshot.forEach(doc => {\r\n                var salon = []\r\n                /* var date = doc.get('createsDate').toDate() */\r\n                var strDate = ''\r\n                /* var tYear = date.getFullYear()\r\n                var tMonth = date.getMonth()\r\n                var tDate = date.getDate() */\r\n                strDate = this.FormatDate(doc.get('createsDate').toDate())\r\n                salon['id'] = doc.id.toString()\r\n                salon['name'] = doc.get('name')\r\n                salon['upDate'] = strDate\r\n                salon['features'] = doc.get('features')\r\n                salon['prefecture'] = doc.get('prefecture')\r\n                this.arrSalons.push(salon)\r\n            })\r\n        })\r\n    },\r\n    methods: {\r\n        FormatDate: function (date) {\r\n            var rtnDate = ''\r\n            var tYear = date.getFullYear()\r\n            var tMonth = date.getMonth()\r\n            var tDate = date.getDate()\r\n            rtnDate = 'upDate ' + tYear + '-' + tMonth + '-' + tDate\r\n            return rtnDate\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style module>\r\n    .flexPickUpsContainer {\r\n    display: flex;\r\n    align-items: flex-start;\r\n    }\r\n\r\n    .pickUpsChild {\r\n    border: 1px solid;\r\n    border-color: pink;\r\n    }\r\n</style>"]}]}