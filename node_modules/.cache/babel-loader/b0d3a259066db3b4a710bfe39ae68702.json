A,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA,oBAAA,IAAA,EAAA;AACA,UAAA,OAAA,GAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,WAAA,EAAA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,QAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,OAAA,EAAA;AACA,MAAA,OAAA,GAAA,YAAA,KAAA,GAAA,GAAA,GAAA,MAAA,GAAA,GAAA,GAAA,KAAA;AACA,aAAA,OAAA;AACA,KARA;AASA,IAAA,cAAA,EAAA,wBAAA,QAAA,EAAA;AACA,UAAA,WAAA,GAAA,EAAA;AACA,MAAA,QAAA,CAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,QAAA,WAAA,IAAA,KAAA,GAAA,GAAA;AACA,OAFA;AAGA,MAAA,WAAA,GAAA,WAAA,CAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,aAAA,WAAA;AACA;AAhBA;AAzBA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"flexPickUpsContainer\">\r\n            <div v-for=\"item in arrSalons\" v-bind:key=\"item.id\" class=\"pickUpsChild col-12 col-sm-2 pr-3 mr-3 mb-2\">\r\n                <div>\r\n                    <p class=\"smallTxt\">{{ item.upDate }}</p>\r\n                    <img src=\"@/img/salon-no-image.jpg\" class=\"img-fluid\">\r\n                    <p class=\"salon-title\">{{ item.name }}</p>\r\n                    <p>{{ item.features }}</p>\r\n                    <p>{{ item.prefecture }}  {{ item.subArea }}</p>\r\n                </div>\r\n            </div>\r\n         </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport db from '@/firebase/firestore'\r\n\r\nexport default {\r\n    name: 'PickupSalon',\r\n    data () {\r\n        return {\r\n            arrSalons: []\r\n        }\r\n    },\r\n    created () {\r\n        /* サロンデータの取得 */\r\n        var docRef = db.collection('salons')\r\n        docRef.orderBy(\"upDate\", \"desc\").limit(4).get().then(DocumentSnapshot => {\r\n            DocumentSnapshot.forEach(doc => {\r\n                var salon = []\r\n                var strDate = ''\r\n                strDate = this.FormatDate(doc.get('upDate').toDate())\r\n                salon['id'] = doc.id.toString()\r\n                salon['name'] = doc.get('name')\r\n                salon['upDate'] = strDate\r\n                salon['features'] = this.FormatFeatures(doc.get('features'))\r\n                salon['prefecture'] = doc.get('prefecture')['name']\r\n                salon['subArea'] = doc.get('subArea')\r\n                this.arrSalons.push(salon)\r\n            })\r\n        })\r\n    },\r\n    methods: {\r\n        FormatDate: function (date) {\r\n            var rtnDate = ''\r\n            var tYear = date.getFullYear()\r\n            var tMonth = date.getMonth()\r\n            var tDate = date.getDate()\r\n            rtnDate = 'upDate ' + tYear + '-' + tMonth + '-' + tDate\r\n            return rtnDate\r\n        },\r\n        FormatFeatures: function (features) {\r\n            var rtnFeatures = ''\r\n            features.forEach ( function( value ){\r\n                rtnFeatures += value + '／'\r\n            })\r\n            rtnFeatures = rtnFeatures.slice(0, -1)\r\n            return rtnFeatures\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n    .flexPickUpsContainer {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n    }\r\n\r\n    .pickUpsChild {\r\n        border: 1px solid;\r\n        border-color: pink;\r\n        align-items: flex-start;\r\n    }\r\n\r\n    .salon-title {\r\n        font-size: large;\r\n        margin-top: 2px;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}